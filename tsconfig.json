{
  "compilerOptions": {
    "jsxImportSource": "react",
    "types": [
      "react",
      "react-dom"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/Homepage.tsx",
    "components/LoadingSkeleton.tsx",
    "components/ProfileDetails.tsx",
    "components/animata/container/Marquee.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
